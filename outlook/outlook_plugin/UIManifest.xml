<?xml version="1.0" encoding="utf-8" ?>
<customUI xmlns="http://schemas.microsoft.com/office/2009/07/customui" loadImage="GetImage">
  <ribbon startFromScratch="false">
    <tabs>
      <tab idMso="TabMail">
        <group id="outcallGroupMail" getLabel="GetLabel" insertAfterMso="GroupNew">
          <button id="outcallMail" getLabel="GetLabel" image="Icon" size="large" onAction="ButtonClicked"/>
        </group>
      </tab>
      <tab idMso="TabContacts">
        <group id="outcallGroupContacts" getLabel="GetLabel" insertAfterMso="GroupNew">
          <button id="outcallContacts" getLabel="GetLabel" image="Icon" size="large" onAction="ButtonClicked"/>
        </group>
      </tab>
    </tabs>
  </ribbon>
  <contextMenus>
    <contextMenu idMso="ContextMenuContactItem">
      <menuSeparator id="a"/>
      <button id="outcallMenuContact" getLabel="GetLabel" onAction="ButtonClicked" image="Icon" />
    </contextMenu>
    <contextMenu idMso="ContextMenuMailItem">
      <menuSeparator id="b"/>
      <button id="outcallMenuMail" getLabel="GetLabel" onAction="ButtonClicked" image="Icon" />
    </contextMenu>
  </contextMenus>
</customUI>